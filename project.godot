; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Living Board"
run/main_scene="res://scenes/ui/title.tscn"
config/features=PackedStringArray("4.2")
config/icon="res://assets/images/icon.png"

[autoload]

InventoryMgr="*res://utils/inventory/inventory_mgr.tscn"
GameInit="*res://Global/game_init.gd"
FullScreenMgr="*res://utils/full_screen_mgr/full_screen_mgr.tscn"
GameStateService="*res://addons/game_state_helper/game_state_service.gd"
TransitionMgr="*res://addons/transition_mgr/transition_mgr.tscn"
Game_Manager="*res://Global/Game_Manager.gd"
Board_Manipulator="*res://Game/Core/rooms/board_manipulator.gd"
Events="*res://Global/Events.gd"
Tween_Utilities="*res://Global/Tween_Utilities.gd"

[debug]

gdscript/warnings/exclude_addons=false

[display]

window/size/viewport_width=1024
window/size/viewport_height=600
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/game_state_helper/plugin.cfg", "res://addons/transition_mgr/plugin.cfg")

[file_customization]

folder_colors={
"res://Game/": "yellow",
"res://Global/": "purple",
"res://Music/": "gray",
"res://addons/": "red",
"res://assets/": "orange",
"res://scenes/": "teal",
"res://scripts/": "blue",
"res://utils/": "pink"
}

